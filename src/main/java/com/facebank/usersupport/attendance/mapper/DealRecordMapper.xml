<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.facebank.usersupport.attendance.mapper.DealRecordMapper">

    <insert id="saveDealRecord" parameterType="com.facebank.usersupport.attendance.model.DealRecordModel">
        INSERT INTO deal_record (apply_id,work_number,deal_level)
        VALUES (#{applyId},#{workNumber},#{dealLevel})
    </insert>

</mapper>